#include <stdio.h>
int max_of_four(int n1,int n2, int n3, int n4){
    if((n1>n2) && (n1>n2) && (n1>n3) && (n1>n4)){
        printf("%d",n1);
    }
    else if((n2>n3)&&(n2>n4)){
        printf("%d",n2);
    }
    else if(n3>n4){
        printf("%d",n3);
    }
    else{
        printf("%d",n4);
    }
    return 0;
}

int main() {
    int a, b, c, d;
    scanf("%d %d %d %d", &a, &b, &c, &d);
    max_of_four(a, b, c, d);
    
    return 0;
}
